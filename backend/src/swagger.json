{
  "openapi": "3.0.0",
  "info": {
    "title": "Node Project API",
    "version": "1.0.0",
    "description": "API documentation for Node Project"
  },
  "paths": {
    "/api/films": {
      "get": {
        "summary": "Get all films",
        "responses": {
          "200": {
            "description": "A list of films",
            "content": {
              "application/json": {
                "schema": {
                  "type": "array",
                  "items": {
                    "type": "object",
                    "properties": {
                      "id": { "type": "integer" },
                      "title": { "type": "string" },
                      "description": { "type": "string" },
                      "rating": { "type": "integer" }
                    }
                  }
                }
              }
            }
          }
        }
      }
    },
    "/api/users": {
      "get": {
        "summary": "Get all users",
        "responses": {
          "200": {
            "description": "A list of users",
            "content": {
              "application/json": {
                "schema": {
                  "type": "array",
                  "items": {
                    "type": "object",
                    "properties": {
                      "id": { "type": "integer" },
                      "name": { "type": "string" },
                      "email": { "type": "string" }
                    }
                  }
                }
              }
            }
          }
        }
      }
    }
  }
}
